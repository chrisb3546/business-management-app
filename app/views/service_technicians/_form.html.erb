<% if @service_technician.errors.any? %>
<ul>
  <% @service_technician.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>
<% end %>
<%=form_for @service_technician do |f|%>
<div class="form-group">
<%=f.label :name%>
<%=f.text_field :name, class:'form-control'%>
<%=f.hidden_field :user_id%>
</div>
<%=f.submit class:'btn-dark btn-lg'%>
<%end%>