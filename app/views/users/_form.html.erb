<% if @user.errors.any? %>
<ul>
  <% @user.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>
<% end %>
<%=form_for @user do |f|%>
<%=f.label :username%>
<%=f.text_field :username%>
<br>
<%=f.label :business_name%>
<%=f.text_field :business_name%>
<br>
<%=f.label :email%>
<%=f.email_field :email%>
<br>
<%=f.label :password%>
<%=f.password_field :password%>
<br>
<%=f.submit%>
<%end%>